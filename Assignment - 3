//program to check the number is odd or even
#include <iostream>

using namespace std;
class number
{
    int a,result;
public:
    int check(int a);
    };

        int number::check(int a)
        {

        if(a%2==0)

            cout<<"even";

        else

         cout<<"odd";


    }

int main()
{
    int a;
    number s;
    cout<<"enter the value of a"<<endl;
    cin>>a;
    s.check(a);
    return 0;
}
