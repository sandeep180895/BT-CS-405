
/* write a program to show multilevel inheritance*/
#include <iostream>
using namespace std;
class x
{
	public:
		int a;
		void data1()
		{ cout<<"enter the value of a - "<<endl;
		  cin>>a;
		}
};

class y:public x
{ 
	public:
		int b;
		void data2()
		{ cout<<"enter the value of a - "<<endl;
		  cin>>b;
		}
};
class z: public y
{ public:
	int m;
	int c;
	void got()
	
		{ cout<<"enter the value of a - "<<endl;
		  cin>>b;
		}
	void multiply()
	{ m=a*b*c;
	cout<<"multiply of three numbers is - "<<m<<endl;
		}	
};
int main()
 {
 	z s;
 	s.data1();
 	s.data2();
 	s.got();
 	s.multiply();
	return 0;
}
